{
  "type": "project",
  "license": "proprietary",
  "authors": [
    {
      "name": "Plenta.io",
      "homepage": "https://plenta.io"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/conflicts": "@dev",
    "contao/manager-bundle": "4.13.*",
    "jrgregory/m17-sticky-backend-footer": "^2.0",
    "terminal42/contao-easy_themes": "^3.0.2",
    "codefog/contao-mobile_menu": "^2.7.1",
    "erdmannfreunde/contao-grid-bundle": "^1.0.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.18",
    "contao/test-case": "^4.6.0",
    "squizlabs/php_codesniffer": "^3.3",
    "friendsofphp/php-cs-fixer": "^v3.7.0",
    "symfony/phpunit-bridge": "^v5.4.3",
    "symfony/webpack-encore-bundle": "^1.11"
  },
  "autoload": {
    "classmap": [
      "src/ContaoManager/Plugin.php"
    ],
    "psr-4": {
      "Plenta\\Customer\\": "src"
    }
  },
  "conflict": {
    "contao-components/installer": "<1.3"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "contao-community-alliance/composer-plugin": true,
      "contao-components/installer": true,
      "contao/manager-plugin": true
    }
  },
  "extra": {
    "contao-component-dir": "assets"
  },
  "scripts": {
    "post-install-cmd": [
      "@php vendor/bin/contao-setup"
    ],
    "post-update-cmd": [
      "@php vendor/bin/contao-setup"
    ]
  }
}
